{
  "expo": {
    "name": "JustLift",
    "slug": "TIGRINHO",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/JUSTLIFT.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#b1ffa3",
        "foregroundImage": "./assets/images/JUSTLIFT.png",
        "backgroundImage": "./assets/images/JUSTLIFT.png",
        "monochromeImage": "./assets/images/JUSTLIFT.png"
      },
      "usesCleartextTraffic": true,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gabrielvicentm.app_treino"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/JUSTLIFT.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin"
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/JUSTLIFT.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-video",
      "expo-font",
      "expo-image"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b4280e36-bede-4e43-a6cd-20efacd69eb0"
      }
    }
  }
}
